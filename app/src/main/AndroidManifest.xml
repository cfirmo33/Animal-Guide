<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.marcelo.animalguide">

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission
        android:name="android.permission.READ_PHONE_STATE"
        tools:node="remove" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".activitys.google_maps_activity.GoogleMapsONGActivity"></activity>
        <activity android:name=".activitys.menus_activity.MenuNavigationONGActivity" />
        <activity android:name=".activitys.notifications_activity.NotificationsONGActivity" />
        <activity android:name=".activitys.searchs_activity.SearchONGActivity" />
        <activity android:name=".activitys.google_maps_activity.GoogleMapsVeterinaryActivity" />
        <activity android:name=".activitys.searchs_activity.SearchVeterinaryActivity" />
        <activity android:name=".activitys.menus_activity.MenuNavigationVeterinaryActivity" />
        <activity android:name=".activitys.notifications_activity.NotificationsVeterinaryActivity" />
        <activity android:name=".activitys.google_maps_activity.GoogleMapsStudentActivity" />
        <activity android:name=".activitys.notifications_activity.NotificationsStudentActivity" />
        <activity android:name=".activitys.searchs_activity.SearchStudentActivity" />
        <activity android:name=".activitys.menus_activity.MenuNavigationStudentActivity" />
        <activity android:name=".activitys.notifications_activity.NotificationsOwnerActivity" />
        <activity android:name=".activitys.google_maps_activity.GoogleMapsOwnerActivity" />
        <activity android:name=".activitys.searchs_activity.SearchOwnerActivity" />
        <activity android:name=".activitys.menus_activity.MenuNavigationOwnerActivity" />
        <activity android:name=".activitys.login.AccountsActivity" />
        <activity android:name=".activitys.splash_screen.SplashScreenActivity" />
        <activity android:name=".activitys.main_activitys.OwnerMainActivity" />
        <activity android:name=".activitys.conexion.CheckInternetActivity" />
        <activity android:name=".activitys.main_activitys.ONGMainActivity" />
        <activity android:name=".activitys.main_activitys.StudentMainActivity" />
        <activity android:name=".activitys.main_activitys.VeterinaryMainActivity" />
        <activity
            android:name=".activitys.password.ForgotPasswordActivity"
            android:parentActivityName=".activitys.login.LoginActivity" />
        <activity
            android:name=".registers.RegisterONGActivity"
            android:parentActivityName=".activitys.login.ChooseActivity" />
        <activity
            android:name=".registers.RegisterVeterinaryActivity"
            android:parentActivityName=".activitys.login.ChooseActivity" />
        <activity
            android:name=".registers.RegisterStudentActivity"
            android:parentActivityName=".activitys.login.ChooseActivity" />
        <activity
            android:name=".registers.RegisterOwnerActivity"
            android:parentActivityName=".activitys.login.ChooseActivity" />
        <activity android:name=".activitys.login.ChooseActivity" />
        <activity android:name=".activitys.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>